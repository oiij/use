{
  "name": "@oiij/three-js",
  "type": "module",
  "version": "0.0.9",
  "description": "A Vue Composable for Three.js",
  "author": "oiij",
  "license": "MIT",
  "homepage": "https://github.com/oiij/use",
  "repository": {
    "type": "git",
    "url": "git@github.com:oiij/use.git"
  },
  "bugs": "https://github.com/oiij/use/issues",
  "keywords": [
    "@oiij/use",
    "three-js"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.mts",
      "import": "./dist/plugins/index.mjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.mts",
      "import": "./dist/utils/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsc --noEmit && tsdown",
    "bumpp": "bumpp --no-push",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "@tweakpane/core": "catalog:",
    "@tweakpane/plugin-essentials": "catalog:",
    "@tweenjs/tween.js": "catalog:",
    "@vueuse/core": "catalog:",
    "cannon-es": "catalog:",
    "mitt": "catalog:",
    "postprocessing": "catalog:",
    "three": "catalog:",
    "tweakpane": "catalog:",
    "vue": "catalog:"
  },
  "devDependencies": {
    "@tweakpane/core": "catalog:",
    "@tweakpane/plugin-essentials": "catalog:",
    "@tweenjs/tween.js": "catalog:",
    "@types/three": "catalog:",
    "@vueuse/core": "catalog:",
    "cannon-es": "catalog:",
    "mitt": "catalog:",
    "postprocessing": "catalog:",
    "three": "catalog:",
    "tweakpane": "catalog:",
    "vue": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
