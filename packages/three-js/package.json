{
  "name": "@oiij/three-js",
  "type": "module",
  "version": "0.0.2",
  "description": "A Vue Composable for Three.js",
  "author": "oiij",
  "license": "MIT",
  "homepage": "https://github.com/oiij/use",
  "repository": {
    "type": "git",
    "url": "git@github.com:oiij/use.git"
  },
  "bugs": "https://github.com/oiij/use/issues",
  "keywords": [
    "@oiij/use",
    "three-js"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.ts",
      "import": "./dist/plugins/index.js",
      "require": "./dist/plugins/index.cjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsc --noEmit && tsdown",
    "bumpp": "bumpp --no-push",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "@tweakpane/core": "^2.0.5",
    "@tweakpane/plugin-essentials": "^0.2.1",
    "@tweenjs/tween.js": "^25.0.0",
    "@vueuse/core": "^13.1.0",
    "cannon-es": "^0.20.0",
    "mitt": "^3.0.1",
    "postprocessing": "^6.37.4",
    "three": "^0.175.0",
    "tweakpane": "^4.0.5",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@tweakpane/core": "^2.0.5",
    "@tweakpane/plugin-essentials": "^0.2.1",
    "@tweenjs/tween.js": "^25.0.0",
    "@types/three": "^0.177.0",
    "@vueuse/core": "^13.3.0",
    "cannon-es": "^0.20.0",
    "mitt": "^3.0.1",
    "postprocessing": "^6.37.4",
    "three": "^0.177.0",
    "tweakpane": "^4.0.5",
    "vue": "^3.5.16"
  },
  "publishConfig": {
    "access": "public"
  }
}
