{
  "name": "@oiij/naive-ui",
  "type": "module",
  "version": "0.0.64",
  "description": "Some Composable Functions And Components for Vue 3",
  "author": "oiij",
  "license": "MIT",
  "homepage": "https://github.com/oiij/use",
  "repository": {
    "type": "git",
    "url": "git@github.com:oiij/use.git"
  },
  "bugs": "https://github.com/oiij/use/issues",
  "keywords": [
    "@oiij/use",
    "naive-ui"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./components": {
      "types": "./dist/components.d.ts",
      "import": "./dist/components.js"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "dev:vite": "vite build --watch",
    "build": "vue-tsc --noEmit && tsdown && vite build",
    "bumpp": "bumpp --no-push",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "@oiij/css-render": "workspace:*",
    "@oiij/markdown-it": "workspace:*",
    "@oiij/use": "workspace:*",
    "@vueuse/core": "catalog:",
    "async-validator": "catalog:",
    "colord": "catalog:",
    "es-toolkit": "catalog:",
    "naive-ui": "catalog:",
    "prismjs": "catalog:",
    "vue": "catalog:",
    "vue-hooks-plus": "catalog:"
  },
  "devDependencies": {
    "@oiij/css-render": "workspace:*",
    "@oiij/markdown-it": "workspace:*",
    "@oiij/use": "workspace:*",
    "@types/prismjs": "catalog:",
    "@vueuse/core": "catalog:",
    "async-validator": "catalog:",
    "colord": "catalog:",
    "es-toolkit": "catalog:",
    "naive-ui": "catalog:",
    "prismjs": "catalog:",
    "vue": "catalog:",
    "vue-hooks-plus": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
