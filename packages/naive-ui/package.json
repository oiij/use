{
  "name": "@oiij/naive-ui",
  "type": "module",
  "version": "0.0.63",
  "description": "Some Composable Functions And Components for Vue 3",
  "author": "oiij",
  "license": "MIT",
  "homepage": "https://github.com/oiij/use",
  "repository": {
    "type": "git",
    "url": "git@github.com:oiij/use.git"
  },
  "bugs": "https://github.com/oiij/use/issues",
  "keywords": [
    "@oiij/use",
    "naive-ui"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./components": {
      "types": "./dist/components.d.ts",
      "import": "./dist/components.js",
      "require": "./dist/components.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "dev:vite": "vite build --watch",
    "build": "vue-tsc --noEmit && tsdown && vite build",
    "bumpp": "bumpp --no-push",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "@oiij/css-render": "workspace:*",
    "@oiij/markdown-it": "workspace:*",
    "@oiij/use": "workspace:*",
    "@vueuse/core": "^14.0.0",
    "async-validator": "^4.2.5",
    "colord": "^2.9.3",
    "es-toolkit": "^1.41.0",
    "naive-ui": "^2.43.1",
    "prismjs": "^1.30.0",
    "vue": "^3.5.22",
    "vue-hooks-plus": "^2.4.1"
  },
  "devDependencies": {
    "@oiij/css-render": "workspace:*",
    "@oiij/markdown-it": "workspace:*",
    "@oiij/use": "workspace:*",
    "@types/prismjs": "^1.26.5",
    "@vueuse/core": "^14.0.0",
    "async-validator": "^4.2.5",
    "colord": "^2.9.3",
    "es-toolkit": "^1.41.0",
    "naive-ui": "^2.43.1",
    "prismjs": "^1.30.0",
    "vue": "^3.5.22",
    "vue-component-type-helpers": "^3.1.2",
    "vue-hooks-plus": "^2.4.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
