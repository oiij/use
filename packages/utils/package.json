{
  "name": "@oiij/utils",
  "type": "module",
  "version": "0.0.5",
  "description": " A collection of utility functions for JavaScript and TypeScript.",
  "author": "oiij",
  "license": "MIT",
  "homepage": "https://github.com/oiij/use",
  "repository": {
    "type": "git",
    "url": "git@github.com:oiij/use.git"
  },
  "bugs": "https://github.com/oiij/use/issues",
  "keywords": [
    "@oiij/use",
    "utils"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./array": {
      "types": "./dist/array.d.mts",
      "import": "./dist/array.mjs"
    },
    "./color": {
      "types": "./dist/color.d.mts",
      "import": "./dist/color.mjs"
    },
    "./date": {
      "types": "./dist/date.d.mts",
      "import": "./dist/date.mjs"
    },
    "./dom": {
      "types": "./dist/dom.d.mts",
      "import": "./dist/dom.mjs"
    },
    "./is": {
      "types": "./dist/is.d.mts",
      "import": "./dist/is.mjs"
    },
    "./math": {
      "types": "./dist/math.d.mts",
      "import": "./dist/math.mjs"
    },
    "./number": {
      "types": "./dist/number.d.mts",
      "import": "./dist/number.mjs"
    },
    "./object": {
      "types": "./dist/object.d.mts",
      "import": "./dist/object.mjs"
    },
    "./random": {
      "types": "./dist/random.d.mts",
      "import": "./dist/random.mjs"
    },
    "./string": {
      "types": "./dist/string.d.mts",
      "import": "./dist/string.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "package.mjson"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsc --noEmit && tsdown",
    "bumpp": "bumpp --no-push",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {},
  "dependencies": {},
  "devDependencies": {},
  "publishConfig": {
    "access": "public"
  }
}
