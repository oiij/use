{
  "name": "@oiij/tiptap",
  "type": "module",
  "version": "0.0.4",
  "description": "A Vue Composable for Tiptap",
  "author": "oiij",
  "license": "MIT",
  "homepage": "https://github.com/oiij/use",
  "repository": {
    "type": "git",
    "url": "git@github.com:oiij/use.git"
  },
  "bugs": "https://github.com/oiij/use/issues",
  "keywords": [
    "@oiij/use",
    "tiptap"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsc --noEmit && tsdown",
    "bumpp": "bumpp --no-push",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "@tiptap/core": "^3.0.9",
    "@tiptap/pm": "^3.0.9",
    "@tiptap/starter-kit": "^3.0.9",
    "@vueuse/core": "^13.6.0",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@tiptap/core": "^3.0.9",
    "@tiptap/pm": "^3.0.9",
    "@tiptap/starter-kit": "^3.0.9",
    "@vueuse/core": "^13.6.0",
    "vue": "^3.5.18"
  },
  "publishConfig": {
    "access": "public"
  }
}
