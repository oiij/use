{
  "name": "@oiij/use",
  "type": "module",
  "version": "0.0.28",
  "description": "Som Composable Functions for Vue 3",
  "author": "oiij",
  "license": "MIT",
  "homepage": "https://github.com/oiij/use",
  "repository": {
    "type": "git",
    "url": "git@github.com:oiij/use.git"
  },
  "bugs": "https://github.com/oiij/use/issues",
  "keywords": [
    "@oiij/use"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./class": {
      "types": "./dist/class.d.mts",
      "import": "./dist/class.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsc --noEmit && tsdown",
    "bumpp": "bumpp --no-push",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "@vueuse/core": "catalog:",
    "vue": "catalog:"
  },
  "devDependencies": {
    "@vueuse/core": "catalog:",
    "vue": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
